--- title: Facet ---
<div class="col-md-12 visual">
    <image src={{img}} alt={{alt}}></image>
</div>
<section class="description facet">

    <div class="facet">
        <h2>{{title}}</h2>
        <p>{{desc}}</p>
    </div>

</section>
<section class="outward facet">
    <div class="inner">
      {{#each links}}
        <div class=col-md-4>
            <a href={{link}} target="_blank"><img src={{img}} alt={{alt}}>
                <h3>{{text}}</h3></a>
        </div>
        {{/each}}
    </div>
</section>
<div class="media col-md-6">
    <img src={{supplementary.img}} alt={{supplementary.img}}>

</div>
<div class="facet col-md-6">
    <h2>{{supplementary.title}}</h2>
    <p>{{supplementary.desc}}
</div>
<section id="work" class="other">
    {{>_footer}}
</section>
